{
    "type": "pre-macro",
    "name": "pre/redefinable-classes",
    "require": {
        "pre/plugin": "^0.7"
    },
    "autoload": {
        "files": [
            "src/expanders.php"
        ],
        "psr-4": {
            "Pre\\RedefinableClasses\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Pre\\RedefinableClasses\\": "tests"
        }
    },
    "extra": {
        "macros": [
            "src/macros.yay"
        ]
    }
}
